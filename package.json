{
  "name": "openapi-eller",
  "version": "0.3.1",
  "description": "Generate OpenAPI v3 clients and servers from the command line",
  "main": "dist/main.js",
  "bin": {
    "openapi-eller": "dist/main.js"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "transpile": "babel src -d dist/ --extensions=\".ts\" --copy-files",
    "build": "npm run transpile",
    "ybuild": "yarn run transpile",
    "msbuild": "npm run transpile",
    "clean": "rm -rf dist && rm-rf tests-dist",
    "test": "tsc -p tests && ava tests",
    "tst": "babel tests -d tests-dist --extensions=\".ts\" --copy-files && ava tests-dist"
  },
  "author": "Brendan Molloy <brendan@bbqsrc.net>",
  "license": "ISC",
  "dependencies": {
    "@types/object-path": "^0.9.29",
    "commander": "^2.15.1",
    "handlebars": "^4.0.11",
    "js-yaml": "^3.10.0",
    "json-refs": "^3.0.6",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "object-path": "^0.11.4",
    "whatwg-url": "^6.4.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/handlebars": "^4.0.37",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "@types/winston": "^2.3.9",
    "ava": "^0.25.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "openapi3-ts": "^0.9.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "openapi",
    "generator",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/technocreatives/openapi-eller.git"
  },
  "engines": {
    "node": "^10.0.0"
  }
}
